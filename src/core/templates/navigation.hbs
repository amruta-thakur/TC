{{! make the _globals object in course.json available to this template}}
{{import_globals}}
{{! All links in the nav bar should have an attribute of data-event, navigationView uses this to fire a global event}}
<div class="navigation-inner clearfix">
    {{#if _accessibility._isEnabled}}
        <button id="accessibility-toggle" class="base icon icon-accessibility a11y-ignore-focus"></button>
        <span id="accessibility-instructions" class="aria-label" role="region" tabindex="0"></span>
    {{/if}}
    {{{a11y_aria_label _globals._accessibility._ariaLabels.navigation}}}
    {{#if _accessibility._isSkipNavigationEnabled}}
        <button class="skip-nav-link a11y-ignore a11y-ignore-focus" data-event="skipNavigation" aria-label="{{_globals._accessibility._ariaLabels.skipNavigation}}">{{{_globals._accessibility.skipNavigationText}}}</button>
    {{/if}}
    <button class="base navigation-back-button icon icon-controls-small-left" data-event="backButton" aria-label="{{_globals._accessibility._ariaLabels.previous}}"></button>
    {{!-- <div class="course-logo" data-event="homeButton">
        
    </div> --}}
    <!--<div class="back-btn"> <a href="" data-event="backButton">Back</a></div>-->

    <!--<div class="navigation-label">Introduction to Wella Color Master BRONZE</div>-->
    <span class="navigation-label page-title"><h1></h1></span>

    <button class="base navigation-drawer-toggle-button resources-icon" data-event="toggleDrawer" aria-label="{{_globals._accessibility._ariaLabels.navigationDrawer}}"> <img src="course/en/images/resources.png"></button>
    <span class="programme_overview"><div class="pv"><a href="course/en/images/pirogramme-overview.html" target="_blank"><img src="course/en/images/po-logo.png"></a></div></span>
</div>
